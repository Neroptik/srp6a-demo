# $OPENSHIFT_DIY_PORT which is usually 8080
#
nohup java -Djetty.host=${OPENSHIFT_DIY_IP} -Djetty.port=${OPENSHIFT_DIY_PORT} -jar ${OPENSHIFT_REPO_DIR}target/dependency/jetty-runner.jar --port ${OPENSHIFT_DIY_PORT} ${OPENSHIFT_REPO_DIR}/targetthinbus-srp-spring-demo.war > ${OPENSHIFT_LOG_DIR}server.log 2>&1 &
